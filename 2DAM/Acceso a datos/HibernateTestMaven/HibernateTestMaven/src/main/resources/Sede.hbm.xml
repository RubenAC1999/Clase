<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entities.Sede" table="sede">
        <id name="id" type="integer" column="id_sede">
            <generator class="identity"/>
        </id>
        <property name="nombre" column="nom_sede" type="string" not-null="true"/>
    </class>
</hibernate-mapping>